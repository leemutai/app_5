<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linear20BookTourOn"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleblack_902">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnarrowdown"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleblack_900_20">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginEnd="@dimen/_23pxh"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowarrowdown"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowdown"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowdown_white_a700"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowdown_white_a700"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTourRequest"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_89pxh"
                        android:layout_weight="0.9"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_tour_request"
                        style="@style/txtPoppinssemibold18_4" tools:text="@string/lbl_tour_request"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnclose"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyle_3">
            <ImageView android:id="@+id/imageClose"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_94pxh"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_52pxv"
                android:layout_width="@dimen/_94pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_close_blue_a204"
                tools:ignore="ContentDescription" tools:src="@drawable/img_close_blue_a204"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtRequestReceive"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/msg_request_receive"
                style="@style/txtPoppinssemibold20" tools:text="@string/msg_request_receive"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtWereChecking"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/msg_we_re_checking"
                style="@style/txtPoppinsregular14_2" tools:text="@string/msg_we_re_checking"/>
            <TextView android:ellipsize="end" android:id="@+id/txtTime"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_26pxv"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/msg_fri_october_4"
                style="@style/txtPoppinssemibold14" tools:text="@string/msg_fri_october_4"/>
        </LinearLayout>
        <TextView android:ellipsize="end" android:gravity="start"
            android:id="@+id/txtAgentWilltake"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_40pxh"
            android:layout_marginStart="@dimen/_40pxh"
            android:layout_marginTop="@dimen/_30pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@string/msg_agent_will_take"
            style="@style/txtPoppinsregular13_1" tools:text="@string/msg_agent_will_take"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowrectanglethirtyone"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageRectangleThirtyOne"
                android:layout_height="@dimen/_45pxh"
                android:layout_width="@dimen/_45pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_rectangle31}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle31"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnsandeeps"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_5pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_weight="0.8" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSandeepS"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_sandeep_s"
                    style="@style/txtPoppinssemibold13" tools:text="@string/lbl_sandeep_s"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPartner"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_partner"
                    style="@style/txtPoppinsregular11_2" tools:text="@string/lbl_partner"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageArrowright"
                android:layout_height="@dimen/_19pxh"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="@dimen/_9pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowright_bluegray_500"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_bluegray_500"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumncontinue"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_242pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyleblack_2">
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnContinue"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_9pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_9pxv"
                android:singleLine="true"
                android:text="@string/lbl_continue"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_9" tools:text="@string/lbl_continue"/>
        </LinearLayout>
    </LinearLayout>
</layout>
