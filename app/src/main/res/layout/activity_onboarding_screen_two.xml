<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="end"
        android:id="@+id/linear02OnboardingS"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_18pxv"
        android:layout_marginEnd="@dimen/_20pxh"
        android:layout_marginStart="@dimen/_20pxh"
        android:layout_marginTop="@dimen/_51pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnsearch"
            android:layout_height="0dp"
            android:layout_marginStart="@dimen/_10pxh"
            android:layout_weight="1.0"
            android:layout_width="match_parent" android:orientation="vertical">
            <ImageView android:id="@+id/imageSearch"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_260pxh"
                android:layout_marginEnd="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_width="@dimen/_295pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_layer1"
                tools:ignore="ContentDescription" tools:src="@drawable/img_layer1"/>
            <TextView android:id="@+id/txtLanguage"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_marginTop="@dimen/_54pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_8pxh"
                android:text="@string/msg_lorem_ipsum_is"
                style="@style/txtPoppinssemibold25" tools:text="@string/msg_lorem_ipsum_is"/>
            <TextView android:id="@+id/txtLoremIpsumis"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:text="@string/msg_lorem_ipsum_is2"
                style="@style/txtPoppinsregular15" tools:text="@string/msg_lorem_ipsum_is2"/>
            <TextView android:ellipsize="end" android:id="@+id/txtSkip"
                android:layout_gravity="end"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_249pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_skip"
                style="@style/txtPoppinsregular13" tools:text="@string/lbl_skip"/>
        </LinearLayout>
        <FrameLayout android:gravity="start"
            android:id="@+id/frameBottombar"
            android:layout_height="wrap_content" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStackarrowleft"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_width="@dimen/_40pxh" style="@style/groupStylecornerRadius">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_13pxv"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                </FrameLayout>

                <com.asksira.loopingviewpager.indicator.CustomShapePagerIndicator
                    android:id="@+id/indicatorClose"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_weight="0.42"
                    android:layout_width="0dp"
                    app:drawable_normal="@drawable/indicator_unselected_bg_blue_100_radius_8"
                    app:drawable_selected="@drawable/indicator_selected_bg_blue_a700_radius_8"
                    app:indicator_size="@dimen/_11pxh" app:indicator_spacing="@dimen/_10pxh"/>
                <FrameLayout android:id="@+id/frameStackarrowright"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_width="@dimen/_40pxh" style="@style/groupStyleblue_A700cornerRadius">
                    <ImageView android:id="@+id/imageArrowright"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_13pxv"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
                </FrameLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
