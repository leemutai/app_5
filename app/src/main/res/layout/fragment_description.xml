<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <androidx.core.widget.NestedScrollView android:clipToPadding="false"
        android:fillViewport="true"
        android:id="@+id/scrollViewScrollview"
        android:layout_height="match_parent" android:layout_width="match_parent">
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnbathtub"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowsvgvieweroutpu"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="center_horizontal"
                    android:id="@+id/linearColumnsvgvieweroutpu"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.23"
                    android:layout_width="0dp"
                    android:orientation="vertical" style="@style/groupStylewhite_3">
                    <ImageView android:id="@+id/imageSvgvieweroutpu"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_15pxh"
                        android:layout_marginEnd="@dimen/_25pxh"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_svgvieweroutpu}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_svgvieweroutpu"/>
                    <FrameLayout android:id="@+id/frameStacksqft"
                        android:layout_gravity="center_horizontal"
                        android:layout_height="@dimen/_29pxv"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginEnd="@dimen/_25pxh"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_26pxh" style="@style/groupStylewhite_4">
                        <FrameLayout
                            android:id="@+id/frameStackcreatefromfra"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_28pxv"
                            android:layout_marginBottom="@dimen/_1pxv" android:layout_width="@dimen/_26pxh">
                            <View
                                android:background="@drawable/rectangle_bg_white_a700_radius_13_5"
                                android:id="@+id/viewCreatefromfra"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_29pxh" android:layout_width="@dimen/_27pxh"/>
                            <TextView android:ellipsize="end"
                                android:gravity="top|start"
                                android:id="@+id/txt1225"
                                android:layout_gravity="top|start"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@string/lbl_1_225"
                                style="@style/txtPoppinsmedium11_1" tools:text="@string/lbl_1_225"/>
                        </FrameLayout>
                        <TextView android:ellipsize="end"
                            android:gravity="top|start"
                            android:id="@+id/txtSqft"
                            android:layout_gravity="bottom|center"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_3pxh"
                            android:layout_marginStart="@dimen/_4pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/lbl_sqft"
                            style="@style/txtPoppinsregular10" tools:text="@string/lbl_sqft"/>
                    </FrameLayout>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerListpathfifty"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_weight="0.77"
                    android:layout_width="0dp"
                    android:nestedScrollingEnabled="false"
                    android:orientation="horizontal"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    itemSpace="@{@dimen/_10pxh}" tools:itemCount="3" tools:listitem="@layout/row_listpathfifty"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtListingAgent"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_listing_agent"
                style="@style/txtPoppinssemibold14" tools:text="@string/lbl_listing_agent"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowrectanglethirtyone"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageRectangleThirtyOne"
                    android:layout_height="@dimen/_45pxh"
                    android:layout_width="@dimen/_45pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_rectangle31}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle31"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnsandeeps"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_weight="0.68"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSandeepS"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_sandeep_s"
                        style="@style/txtPoppinssemibold13" tools:text="@string/lbl_sandeep_s"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPartner"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_partner"
                        style="@style/txtPoppinsregular11_2" tools:text="@string/lbl_partner"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageTelevision"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_3pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="@dimen/_57pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_television_light_blue_a700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_television_light_blue_a700"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFacilities"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_facilities"
                style="@style/txtPoppinssemibold14" tools:text="@string/lbl_facilities"/>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerListcar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="match_parent"
                android:nestedScrollingEnabled="false"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_15pxh}" tools:itemCount="2" tools:listitem="@layout/row_listcar"/>
            <LinearLayout android:id="@+id/linearRowaddress"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStyle_1">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAddress"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.43"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/lbl_address"
                    style="@style/txtPoppinssemibold14" tools:text="@string/lbl_address"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtViewonMap"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_203pxh"
                    android:layout_weight="0.57"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/lbl_view_on_map"
                    style="@style/txtPoppinsmedium12_1" tools:text="@string/lbl_view_on_map"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowlinkedin"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageLinkedin"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_12pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_linkedin_bluegray_912"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_linkedin_bluegray_912"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_weight="0.95"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/msg_lorem_ipsum_is3"
                    style="@style/txtPoppinsregular13_1" tools:text="@string/msg_lorem_ipsum_is3"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameStackhome"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_201pxv"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_7pxv" android:layout_width="@dimen/_340pxh">
                <ImageView android:id="@+id/imageImageOne"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_201pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_20pxh}"
                    imageRes="@{@drawable/img_image1_201x340}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_image1_201x340"/>
                <ImageView android:id="@+id/imageHome"
                    android:layout_gravity="bottom|center"
                    android:layout_height="@dimen/_36pxh"
                    android:layout_marginBottom="@dimen/_73pxv"
                    android:layout_marginEnd="@dimen/_150pxh"
                    android:layout_marginStart="@dimen/_150pxh"
                    android:layout_marginTop="@dimen/_73pxv"
                    android:layout_width="@dimen/_29pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_home_bluegray_904"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_home_bluegray_904"/>
            </FrameLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowtotalprice"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_35pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_6">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumntotalprice"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_21pxv"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_weight="0.51"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTotalPrice"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_total_price"
                        style="@style/txtPoppinssemibold15_1" tools:text="@string/lbl_total_price"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPrice"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@string/lbl_350_month"
                        style="@style/txtPoppinsregular13_2" tools:text="@string/lbl_350_month"/>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnBookNow"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_weight="0.49"
                    android:layout_width="0dp"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_10pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_book_now"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_2" tools:text="@string/lbl_book_now"/>
            </LinearLayout>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</layout>
