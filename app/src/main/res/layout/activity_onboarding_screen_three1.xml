<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnrectangle557"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleblack_901">
        <FrameLayout android:gravity="start"
            android:id="@+id/frame02OnboardingS"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStyleblack_901">
            <View android:background="@color/black_902"
                android:id="@+id/viewRectangle557"
                android:layout_gravity="start"
                android:layout_height="@dimen/_772pxh" android:layout_width="@dimen/_375pxh"/>
            <LinearLayout android:id="@+id/linearColumnsearch"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageView android:id="@+id/imageSearch"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_260pxh"
                    android:layout_marginEnd="@dimen/_26pxh"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="@dimen/_295pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_layer1"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_layer1"/>
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtDiscoverProper"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_26pxh"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_54pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_8pxh"
                    android:text="@string/msg_discover_proper"
                    style="@style/txtPoppinssemibold25" tools:text="@string/msg_discover_proper"/>
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txtLoremIpsumis"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_26pxh"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:text="@string/msg_lorem_ipsum_is2"
                    style="@style/txtPoppinsregular15_3" tools:text="@string/msg_lorem_ipsum_is2"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSkip"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_20pxh"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_249pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_skip"
                    style="@style/txtPoppinsregular13_3" tools:text="@string/lbl_skip"/>
            </LinearLayout>
        </FrameLayout>
        <FrameLayout android:gravity="center_horizontal"
            android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearRowarrowleft"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStackarrowleft"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_width="@dimen/_40pxh" style="@style/groupStylecornerRadius_9">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_13pxv"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft_blue_a203"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_blue_a203"/>
                </FrameLayout>

                <com.asksira.loopingviewpager.indicator.CustomShapePagerIndicator
                    android:id="@+id/indicatorClose"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_weight="0.42"
                    android:layout_width="0dp"
                    app:drawable_normal="@drawable/indicator_unselected_bg_blue_100_radius_8"
                    app:drawable_selected="@drawable/indicator_selected_bg_blue_a203_radius_8"
                    app:indicator_size="@dimen/_11pxh" app:indicator_spacing="@dimen/_10pxh"/>
                <FrameLayout android:id="@+id/frameStackarrowright"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_width="@dimen/_40pxh" style="@style/groupStyleblue_A203cornerRadius">
                    <ImageView android:id="@+id/imageArrowright"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_13pxv"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright_black_902"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_black_902"/>
                </FrameLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
