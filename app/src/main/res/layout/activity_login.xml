<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linear04Login"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <ImageView android:id="@+id/imageLayerOne"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_64pxh"
            android:layout_marginEnd="@dimen/_25pxh"
            android:layout_marginStart="@dimen/_25pxh"
            android:layout_marginTop="@dimen/_75pxv"
            android:layout_width="@dimen/_260pxh"
            android:scaleType="fitXY" android:src="@drawable/img_layer1"
            tools:ignore="ContentDescription" tools:src="@drawable/img_layer1"/>
        <TextView android:ellipsize="end" android:id="@+id/txtSignIn"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_25pxh"
            android:layout_marginStart="@dimen/_25pxh"
            android:layout_marginTop="@dimen/_61pxv"
            android:layout_width="wrap_content"
            android:singleLine="true" android:text="@string/lbl_sign_in"
            style="@style/txtPoppinssemibold18" tools:text="@string/lbl_sign_in"/>
        <TextView android:ellipsize="end" android:id="@+id/txtUsername"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_25pxh"
            android:layout_marginStart="@dimen/_25pxh"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@string/lbl_username"
            style="@style/txtPoppinsmedium14" tools:text="@string/lbl_username"/>
        <EditText
            android:background="@drawable/rectangle_border_gray_701_radius_10"
            android:drawablePadding="@dimen/_12pxh"
            android:drawableStart="@drawable/img_lock"
            android:ellipsize="end" android:hint="@string/lbl_username"
            android:id="@+id/etFrame117127554" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_25pxh"
            android:layout_marginStart="@dimen/_25pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_13pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_20pxh"
            android:paddingTop="@dimen/_15pxv" android:singleLine="true"
            android:textColorHint="@color/gray_700"
            style="@style/etRoundedOutline" tools:ignore="Autofill"/>
        <TextView android:ellipsize="end" android:id="@+id/txtPassword"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_25pxh"
            android:layout_marginStart="@dimen/_25pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@string/lbl_password"
            style="@style/txtPoppinsmedium14" tools:text="@string/lbl_password"/>
        <ImageView android:id="@+id/imageFrame117127554One"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_48pxh"
            android:layout_marginEnd="@dimen/_25pxh"
            android:layout_marginStart="@dimen/_25pxh"
            android:layout_marginTop="@dimen/_11pxv"
            android:layout_width="@dimen/_324pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_frame117127554"
            tools:ignore="ContentDescription" tools:src="@drawable/img_frame117127554"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnSignInOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_25pxh"
            android:layout_marginStart="@dimen/_25pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_10pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_10pxv" android:singleLine="true"
            android:text="@string/lbl_sign_in"
            android:textAllCaps="false" style="@style/btnSolidRounded_1" tools:text="@string/lbl_sign_in"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowrectanglethree"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_25pxh"
            android:layout_marginStart="@dimen/_25pxh"
            android:layout_marginTop="@dimen/_30pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <View
                android:background="@drawable/rectangle_gradient_s_gray_700_bf_e_gray_700_bf"
                android:id="@+id/lineRectangleThree"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginBottom="@dimen/_7pxv"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_weight="0.34" android:layout_width="0dp"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtOrcontinuewit"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_weight="0.32" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@string/msg_or_continue_wit"
                style="@style/txtPoppinsmedium11" tools:text="@string/msg_or_continue_wit"/>
            <View
                android:background="@drawable/rectangle_gradient_s_gray_700_bf_e_gray_700_bf1"
                android:id="@+id/lineRectangleFour"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginBottom="@dimen/_7pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_weight="0.34" android:layout_width="0dp"/>
        </LinearLayout>
        <ImageView android:id="@+id/imageSettings"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_19pxh"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_25pxh"
            android:layout_marginStart="@dimen/_25pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="@dimen/_95pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_settings"
            tools:ignore="ContentDescription" tools:src="@drawable/img_settings"/>
    </LinearLayout>
</layout>
