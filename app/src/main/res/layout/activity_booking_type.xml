<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linear16BookingType"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnarrowdown"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700_19">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_marginEnd="@dimen/_23pxh"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowarrowdown"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowdown"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowdown"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowdown"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtBooking"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_109pxh"
                        android:layout_weight="0.85"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_booking"
                        style="@style/txtPoppinssemibold20_1" tools:text="@string/lbl_booking"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtBookingOptions"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_26pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@string/lbl_booking_options"
            style="@style/txtPoppinssemibold16" tools:text="@string/lbl_booking_options"/>
        <EditText
            android:background="@drawable/rectangle_border_bluegray_900_cc"
            android:drawableEnd="@drawable/img_richeckboxblankcircleline"
            android:drawablePadding="@dimen/_30pxh"
            android:ellipsize="end"
            android:hint="@string/lbl_property_tour"
            android:id="@+id/etFrame117127548" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_7pxv"
            android:paddingTop="@dimen/_9pxv" android:singleLine="true"
            android:textColorHint="@color/bluegray_900_cc"
            style="@style/etOutline" tools:ignore="Autofill"/>
        <LinearLayout android:id="@+id/linearRowrealestate"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_590pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStyle_2">
            <TextView android:ellipsize="end"
                android:id="@+id/txtRealEstate"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_9pxv"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_weight="0.85" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@string/lbl_real_estate"
                style="@style/txtPoppinsregular14_1" tools:text="@string/lbl_real_estate"/>
            <View
                android:background="@drawable/rectangle_border_bluegray_900_cc_radius_7"
                android:id="@+id/viewEllipseEighteen"
                android:layout_height="@dimen/_14pxh"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginStart="@dimen/_244pxh"
                android:layout_marginTop="@dimen/_12pxv" android:layout_width="@dimen/_14pxh"/>
        </LinearLayout>
    </LinearLayout>
</layout>
