<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnrectanglenine"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="vertical">
        <ImageView android:id="@+id/imageRectangleNine"
            android:layout_height="@dimen/_105pxh"
            android:layout_width="@dimen/_158pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_10pxh}"
            imageRes="@{@drawable/img_rectangle9_105x158}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle9_105x158"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnloremhouse"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_1">
            <TextView android:ellipsize="end"
                android:id="@+id/txtLoremHouse"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_lorem_house"
                style="@style/txtPoppinssemibold12_1" tools:text="@string/lbl_lorem_house"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowprice"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_8pxv"
                android:layout_marginEnd="@dimen/_8pxh"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnprice"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.85"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPrice"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_340_month"
                        style="@style/txtPoppinsregular9" tools:text="@string/lbl_340_month"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowlinkedin"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageLinkedin"
                            android:layout_height="@dimen/_9pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_7pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_linkedin"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_linkedin"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtTimeZone"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_3pxh"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@string/msg_avenue_west_si"
                            style="@style/txtPoppinsregular7" tools:text="@string/msg_avenue_west_si"/>
                    </LinearLayout>
                </LinearLayout>
                <ImageButton
                    android:background="@drawable/rectangle_bg_blue_50_radius_8"
                    android:id="@+id/btnBookmark"
                    android:layout_height="@dimen/_22pxh"
                    android:layout_width="@dimen/_21pxh"
                    android:paddingHorizontal="@dimen/_6pxh"
                    android:paddingVertical="@dimen/_6pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_bookmark" tools:ignore="ContentDescription"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
