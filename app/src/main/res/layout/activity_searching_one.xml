<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linear29SearchingOn"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp" style="@style/groupStylewhite_A700_19">
            <androidx.appcompat.widget.SearchView
                android:background="@drawable/rectangle_bg_gray_101_radius_8"
                android:id="@+id/searchViewFrameEight"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_width="match_parent"
                android:theme="@style/searchViewFrameEight"
                app:iconifiedByDefault="false"
                app:queryBackground="@android:color/transparent"
                app:queryHint="@string/lbl_search" app:searchIcon="@drawable/img_close_blue_a700_14x14"/>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnrecentsearch"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="wrap_content"
            android:orientation="vertical" style="@style/groupStylecornerRadius_8">
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtRecentSearch"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_recent_search"
                style="@style/txtPoppinssemibold16" tools:text="@string/lbl_recent_search"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowserenityheight"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylecornerRadius_8">
            <TextView android:ellipsize="end"
                android:id="@+id/txtSerenityHeight"
                android:layout_height="wrap_content"
                android:layout_weight="0.91" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@string/msg_serenity_height"
                style="@style/txtPoppinsregular14_4" tools:text="@string/msg_serenity_height"/>
            <ImageView android:id="@+id/imageClose"
                android:layout_height="@dimen/_11pxh"
                android:layout_marginBottom="@dimen/_6pxv"
                android:layout_marginStart="@dimen/_211pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="@dimen/_11pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_close_gray_800"
                tools:ignore="ContentDescription" tools:src="@drawable/img_close_gray_800"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowwillowhaven"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylecornerRadius_8">
            <TextView android:ellipsize="end"
                android:id="@+id/txtWillowHaven"
                android:layout_height="wrap_content"
                android:layout_weight="0.89" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@string/lbl_willow_haven"
                style="@style/txtPoppinsregular14_4" tools:text="@string/lbl_willow_haven"/>
            <ImageView android:id="@+id/imageCloseOne"
                android:layout_height="@dimen/_11pxh"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_marginStart="@dimen/_231pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="@dimen/_11pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_close_gray_800"
                tools:ignore="ContentDescription" tools:src="@drawable/img_close_gray_800"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowazureskyline"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylecornerRadius_8">
            <TextView android:ellipsize="end"
                android:id="@+id/txtAzureSkyline"
                android:layout_height="wrap_content"
                android:layout_weight="0.89" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@string/lbl_azure_skyline"
                style="@style/txtPoppinsregular14_4" tools:text="@string/lbl_azure_skyline"/>
            <ImageView android:id="@+id/imageCloseTwo"
                android:layout_height="@dimen/_11pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_233pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="@dimen/_11pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_close_gray_800"
                tools:ignore="ContentDescription" tools:src="@drawable/img_close_gray_800"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnrecentview"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_30pxv"
            android:layout_width="wrap_content"
            android:orientation="vertical" style="@style/groupStylecornerRadius_8">
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtRecentView"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_recent_view"
                style="@style/txtPoppinssemibold16" tools:text="@string/lbl_recent_view"/>
        </LinearLayout>
        <androidx.recyclerview.widget.RecyclerView
            android:gravity="center_horizontal"
            android:id="@+id/recyclerListrectangletwelve"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_44pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_width="match_parent"
            android:orientation="vertical"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            itemSpace="@{@dimen/_18pxh}" tools:itemCount="4" tools:listitem="@layout/row_listrectangletwelve2"/>
    </LinearLayout>
</layout>
